{
  "name": "directorios-empresariales",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "node scripts/build-info.js && vite build",
    "build:local": "node scripts/build-local.js",
    "build:quick": "npm run build",
    "build:check": "npm run lint && npm run test && npm run build",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "preview": "vite preview",
    "clean": "rm -rf dist node_modules/.vite",
    "pre-commit": "npm run build:check"
  },
  "dependencies": {
    "@supabase/supabase-js": "2.38.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.8.1",
    "zustand": "4.3.9"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "14.4.3",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@vitejs/plugin-react": "3.1.0",
    "autoprefixer": "10.4.14",
    "eslint": "8.35.0",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "jsdom": "21.1.1",
    "postcss": "8.4.21",
    "tailwindcss": "3.2.7",
    "typescript": "4.9.5",
    "vite": "4.1.4",
    "vitest": "0.29.7"
  }
}